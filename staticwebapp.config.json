{
  "routes": [
    {
      "route": "/",
      "serve": "/debug-standalone.html"
    },
    {
      "route": "/*",
      "serve": "/debug-standalone.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/debug-standalone.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/debug-standalone.html"
    }
  }
}
